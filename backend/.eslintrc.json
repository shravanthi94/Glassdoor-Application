{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 2],
        "no-console":"off",
        "no-unused-vars":"off",
        "eol-last": 0,
        "linebreak-style": "off",
        "consistent-return": "off"
    }
}
